Msat.SetRegion(0, 0)
m.SetRegion(0, uniform(0,0,0))
Aex.SetRegion(0, 0)
Kc1.SetRegion(0, 0)

Bx := 56.6e-3
By := 56.6e-3
B_ext = vector(Bx,By,0) //diagonal

relax()         // high-energy states best minimized by relax()
saveas(m, "m")

